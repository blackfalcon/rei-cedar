<cdr-list-to-grid>
    <section class="tile">
        <div class="hidden-xs media-frame ratio-3-4 portrait center">
            <img src="{ item.imgPath }"  alt="{ item.imgPathAlt }">
        </div>
        <div class="tile-block">
            <cdr-event-date item={ item } />
        </div>
    </section>
</cdr-list-to-grid>
