.cdr- {
  &h1 {
    font-size: 4.8rem;

    @apply --font-primary;
  }

  &h2 {
    font-size: 4rem;

    @apply --font-secondary;
  }

  &h3 {
    font-size: 3.2rem;

    @apply --font-tertiary;
  }

  &h4 {
    font-size: 2.4rem;
  }

  &h5 {
    font-size: 1.6rem;
  }

  &h6 {
    font-size: 1.2rem;
  }
}
