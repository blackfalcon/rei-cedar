.cdr- {
  &h1 {
    font-size: 48px;

    @apply --font-primary;
  }

  &h2 {
    font-size: 40px;

    @apply --font-secondary;
  }

  &h3 {
    font-size: 32px;

    @apply --font-tertiary;
  }

  &h4 {
    font-size: 24px;
  }

  &h5 {
    font-size: 16px;
  }

  &h6 {
    font-size: 12px;
  }
}
