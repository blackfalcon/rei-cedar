  NOTE: BEM naming except for _row and _span (instead of --row and --col)
   which are used for doing simpler :not() filtering
********************/

/*******************
  row classes
********************/

.cdr-row {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-left: calc(var(--rowGutter) * -1 / 2);
  margin-right: calc(var(--rowGutter) * -1 / 2);
  padding-left: 0;
  padding-right: 0;

  &.cdr-col {
    margin: 0;
    padding: 0;

    &:not([class*="colHide"]) {
      display: flex;
    }
  }

  /* row by number */

  &_row1 {
    & > .cdr-col:not([class*="cdr-col_span"]) {
      @apply --row-1;
    }
  }

  &_row2 {
    & > .cdr-col:not([class*="cdr-col_span"]) {
      @apply --row-2;
    }
  }

  &_row3 {
