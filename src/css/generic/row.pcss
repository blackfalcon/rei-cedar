/*******************
  col classes
********************/

.cdr-col {
  flex: 1 1 0;
  padding-left: calc(var(--rowGutter) / 2);
  padding-right: calc(var(--rowGutter) / 2);
  padding-bottom: var(--colBottom);
  display: flex;

  &__content {
    display: flex;
    flex: 1 1 1px;
    width: 100%;

    & > .cdr-row {
      margin: 0;
      padding: 0;
    }

    & > * {
      height: auto;
      width: inherit;
    }
  }

  /* col by number */

  &_span1 {
