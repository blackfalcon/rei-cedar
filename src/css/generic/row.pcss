  --11-row-pct: calc(var(--rowColumns) / 11 / var(--rowColumns) * 100%);
  --12-row-pct: calc(var(--rowColumns) / 12 / var(--rowColumns) * 100%);
  --row-1: {
    flex-basis: var(--1-row-pct);
    width: var(--1-row-pct);
    max-width: var(--1-row-pct);
  }
  --row-2: {
    flex-basis: var(--2-row-pct);
    width: var(--2-row-pct);
    max-width: var(--2-row-pct);
